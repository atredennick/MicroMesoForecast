@Article{Fowler2012,
  Title                    = {Species dynamics alter community diversityâ€“biomass stability relationships},
  Author                   = {Fowler, Mike S. and Laakso, Jouni and Kaitala, Veijo and Ruokolainen, Lasse and Ranta, Esa},
  Journal                  = {Ecology Letters},
  Year                     = {2012},
  Number                   = {12},
  Pages                    = {1387--1396},
  Volume                   = {15},

  Abstract                 = {The relationship between community diversity and biomass variability remains a crucial ecological topic, with positive, negative and neutral diversityâ€“stability relationships reported from empirical studies. Theory highlights the relative importance of Speciesâ€“Species or Speciesâ€“Environment interactions in driving diversityâ€“stability patterns. Much previous work is based on an assumption of identical (stable) species-level dynamics. We studied ecosystem models incorporating stable, cyclic and more complex species-level dynamics, with either linear or non-linear density dependence, within a locally stable community framework. Species composition varies with increasing diversity, interacting with the correlation of species' environmental responses to drive either positive or negative diversityâ€“stability patterns, which theory based on communities with only stable species-level dynamics fails to predict. Including different dynamics points to new mechanisms that drive the full range of diversityâ€“biomass stability relationships in empirical systems where a wider range of dynamical behaviours are important.},
  Doi                      = {10.1111/j.1461-0248.2012.01862.x},
  File                     = {:Fowler_2012_EcolLEtt.pdf:PDF},
  ISSN                     = {1461-0248},
  Keywords                 = {Complex dynamics, dynamical instability, environmental stochasticity, intrinsic growth rate, portfolio/insurance effect, synchrony, speciesâ€“environment interactions, speciesâ€“species interactions},
  Owner                    = {atredenn},
  Timestamp                = {2013.02.15},
  Url                      = {http://dx.doi.org/10.1111/j.1461-0248.2012.01862.x}
}

@Article{Freckleton2011,
  Title                    = {Density-Structured Models for Plant Population Dynamics.},
  Author                   = {Robert P. Freckleton and William J. Sutherland and Andrew R. Watkinson and Simon A. Queenborough},
  Journal                  = {The American Naturalist},
  Year                     = {2011},

  Month                    = jan,
  Number                   = {1},
  Pages                    = {1--17},
  Volume                   = {177},

  Abstract                 = {Abstract: Densityâ€structured models are structured population models in which the state variable is the proportion of populations or sites in a small number of discrete density states. Although such models have rarely been used, they have the advantage that they are straightforward to parameterize, make few assumptions about population dynamics, and permit rapid data collection using coarse density assessment. In this article, we highlight their use in relating population dynamics to environmental variation and their robustness to measurement error. We show that densityâ€structured models are able to accurately represent population dynamics under a wide range of conditions. We look at the effects of including a persistent seedbank and describe numerical approximations for the mean and variance of population size. For simulated data, we determine the extent to which the underlying continuous process may be inferred from densityâ€structured data. Finally, we discuss issues of parameter estimation and applications for which these types of models may be useful.},
  File                     = {:Freckleton2011.pdf:PDF},
