@Article{Bell2005,
  Title                    = {Larger Islands House More Bacterial Taxa},
  Author                   = {Bell, Thomas and Ager, Duane and Song, Ji-Inn and Newman, Jonathan A. and Thompson, Ian P. and Lilley, Andrew K. and van der Gast, Christopher J.},
  Journal                  = {Science},
  Year                     = {2005},
  Number                   = {5730},
  Pages                    = {1884},
  Volume                   = {308},

  Abstract                 = {The power law that describes the relationship between species richness and area size is one of the few generalizations in ecology, but recent studies show that this relationship differs for microbes. We demonstrate that the natural bacterial communities inhabiting small aquatic islands (treeholes) do indeed follow the species-area law. The result requires a re-evaluation of the current understanding of how natural microbial communities operate and implies that analogous processes structure both microbial communities and communities of larger organisms.}
}

