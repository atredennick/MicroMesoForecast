@Article{Moustakas2013,
  Title                    = {Facilitation or Competition? Tree Effects on Grass Biomass across a Precipitation Gradient},
  Author                   = {Moustakas, Aristides and Kunin, William E. and Cameron, Tom C. and Sankaran, Mahesh},
  Journal                  = {PLoS ONE},
  Year                     = {2013},

  Month                    = feb,
  Number                   = {2},
  Pages                    = {e57025--},
  Volume                   = {8},

  Abstract                 = {<p>Savanna ecosystems are dominated by two distinct plant life forms, grasses and trees, but the interactions between them are poorly understood. Here, we quantified the effects of isolated savanna trees on grass biomass as a function of distance from the base of the tree and tree height, across a precipitation gradient in the Kruger National Park, South Africa. Our results suggest that mean annual precipitation (MAP) mediates the nature of tree-grass interactions in these ecosystems, with the impact of trees on grass biomass shifting qualitatively between 550 and 737 mm MAP. Tree effects on grass biomass were facilitative in drier sites (MAPâ‰¤550 mm), with higher grass biomass observed beneath tree canopies than outside. In contrast, at the wettest site (MAPâ€Š=â€Š737 mm), grass biomass did not differ significantly beneath and outside tree canopies. Within this overall precipitation-driven pattern, tree height had positive effect on sub-canopy grass biomass at some sites, but these effects were weak and not consistent across the rainfall gradient. For a more synthetic understanding of tree-grass interactions in savannas, future studies should focus on isolating the different mechanisms by which trees influence grass biomass, both positively and negatively, and elucidate how their relative strengths change over broad environmental gradients.</p>},
  Doi                      = {10.1371/journal.pone.0057025},
  File                     = {:Moustakas2013.pdf:PDF},
  Owner                    = {atredenn},
  Publisher                = {Public Library of Science},
  Timestamp                = {2013.05.02},
  Url                      = {http://dx.doi.org/10.1371%2Fjournal.pone.0057025}
}

