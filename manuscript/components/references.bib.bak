@Article{OHara2005,
  Title                    = {The anarchist's guide to ecological theory. Or, we don't need no stinkinâ€™ laws},
  Author                   = {O'Hara, R. B.},
  Journal                  = {Oikos},
  Year                     = {2005},
  Number                   = {2},
  Pages                    = {390-393},
  Volume                   = {110},

  Abstract                 = {Several ecologists have recently suggested that ecology has several laws. This conclusion contrasts with the views of some philosophers of science, who have suggested that biology cannot have laws. I argue that the debate has been confused because two very different types of law can be recognised: correlative and causal laws. Once we recognise that there is a difference, the argument against causal laws becomes stronger, and instead I suggest that ecologists should recognise that they can and do produce generalisations that are used to build models â€“ nomological machines â€“ that describe the ecological systems they are studying.}
}

