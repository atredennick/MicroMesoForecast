@Article{Mazancourt2013,
  Title                    = {Predicting ecosystem stability from community composition and biodiversity},
  Author                   = {de Mazancourt, Claire and Isbell, Forest and Larocque, Allen and Berendse, Frank and De Luca, Enrica and Grace, James B. and Haegeman, Bart and Wayne Polley, H. and Roscher, Christiane and Schmid, Bernhard and Tilman, David and van Ruijven, Jasper and Weigelt, Alexandra and Wilsey, Brian J. and Loreau, Michel},
