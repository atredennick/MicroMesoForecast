@Article{Adler2011,
  Title                    = {Productivity Is a Poor Predictor of Plant Species Richness},
  Author                   = {Adler, Peter B. and Seabloom, Eric W. and Borer, Elizabeth T. and Hillebrand, Helmut and Hautier, Yann and Hector, Andy and Harpole, W. Stanley and O'Halloran, Lydia R. and Grace, James B. and Anderson, T. Michael and Bakker, Jonathan D. and Biederman, Lori A. and Brown, Cynthia S. and Buckley, Yvonne M. and Calabrese, Laura B. and Chu, Cheng-Jin and Cleland, Elsa E. and Collins, Scott L. and Cottingham, Kathryn L. and Crawley, Michael J. and Damschen, Ellen I. and Davies, Kendi F. and DeCrappeo, Nicole M. and Fay, Philip A. and Firn, Jennifer and Frater, Paul and Gasarch, Eve I. and Gruner, Daniel S. and Hagenah, Nicole and Hille Ris Lambers, Janneke and Humphries, Hope and Jin, Virginia L. and Kay, Adam D. and Kirkman, Kevin P. and Klein, Julia A. and Knops, Johannes M. H. and La Pierre, Kimberly J. and Lambrinos, John G. and Li, Wei and MacDougall, Andrew S. and McCulley, Rebecca L. and Melbourne, Brett A. and Mitchell, Charles E. and Moore, Joslin L. and Morgan, John W. and Mortensen, Brent and Orrock, John L. and Prober, Suzanne M. and Pyke, David A. and Risch, Anita C. and Schuetz, Martin and Smith, Melinda D. and Stevens, Carly J. and Sullivan, Lauren L. and Wang, Gang and Wragg, Peter D. and Wright, Justin P. and Yang, Louie H.},
  Journal                  = {Science},
  Year                     = {2011},
  Number                   = {6050},
  Pages                    = {1750-1753},
  Volume                   = {333},

  Abstract                 = {For more than 30 years, the relationship between net primary productivity and species richness has generated intense debate in ecology about the processes regulating local diversity. The original view, which is still widely accepted, holds that the relationship is hump-shaped, with richness first rising and then declining with increasing productivity. Although recent meta-analyses questioned the generality of hump-shaped patterns, these syntheses have been criticized for failing to account for methodological differences among studies. We addressed such concerns by conducting standardized sampling in 48 herbaceous-dominated plant communities on five continents. We found no clear relationship between productivity and fine-scale (metersâˆ’2) richness within sites, within regions, or across the globe. Ecologists should focus on fresh, mechanistic approaches to understanding the multivariate links between productivity and richness.}
}

