@Article{Myneni2007,
  Title                    = {Large seasonal swings in leaf area of Amazon rainforests},
  Author                   = {Myneni, Ranga B. and Yang, Wenze and Nemani, Ramakrishna R. and Huete, Alfredo R. and Dickinson, Robert E. and Knyazikhin, Yuri and Didan, Kamel and Fu, Rong and Negr\'{o}n Ju\'{a}rez, Robinson I. and Saatchi, Sasan S. and Hashimoto, Hirofumi and Ichii, Kazuhito and Shabanov, Nikolay V. and Tan, Bin and Ratana, Piyachat and Privette, Jeffrey L. and Morisette, Jeffrey T. and Vermote, Eric F. and Roy, David P. and Wolfe, Robert E. and Friedl, Mark A. and Running, Steven W. and Votava, Petr and El-Saleous, Nazmi and Devadiga, Sadashiva and Su, Yin and Salomonson, Vincent V.},
  Journal                  = {Proceedings of the National Academy of Sciences},
  Year                     = {2007},
  Number                   = {12},
  Pages                    = {4820-4823},
  Volume                   = {104},

  Abstract                 = {Despite early speculation to the contrary, all tropical forests studied to date display seasonal variations in the presence of new leaves, flowers, and fruits. Past studies were focused on the timing of phenological events and their cues but not on the accompanying changes in leaf area that regulate vegetationâ€“atmosphere exchanges of energy, momentum, and mass. Here we report, from analysis of 5 years of recent satellite data, seasonal swings in green leaf area of â‰ˆ25% in a majority of the Amazon rainforests. This seasonal cycle is timed to the seasonality of solar radiation in a manner that is suggestive of anticipatory and opportunistic patterns of net leaf flushing during the early to mid part of the light-rich dry season and net leaf abscission during the cloudy wet season. These seasonal swings in leaf area may be critical to initiation of the transition from dry to wet season, seasonal carbon balance between photosynthetic gains and respiratory losses, and litterfall nutrient cycling in moist tropical forests.}
}

